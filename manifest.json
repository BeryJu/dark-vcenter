{
    "name": "Dark vCenter",
    "version": "0.1",
    "description": "Apply Dark Clarity Theme to vCenter!",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://*/ui/*"
            ],
            "js":[
                "inject.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "css/clr-ui-dark.css",
        "css/fixes.css"
    ]
}